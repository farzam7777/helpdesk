$('#services_section').html('<%= escape_javascript(render @services) %>');
$("#services-count").html("Viewing <%= @services.count %> services (of <%= @services.total_count %> total)")
$("#paginator").html("<%= escape_javascript paginate @services, :theme => 'twitter-bootstrap-3', remote: true, window: 2, outer_window: 3 %>")
<% if @check %>
	$('#title-input').val('')
	$('#type-input').val('')
<% end %>